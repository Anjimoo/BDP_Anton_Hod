<Window x:Class="BDP_Anton_Hod.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BDP_Anton_Hod"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="standartStart" Content="Standart Data Analysis" HorizontalAlignment="Left" VerticalAlignment="Top" Click="button_Click" Height="80" Width="200" Margin="140,60,0,0"/>
        <Button x:Name="MRButton" Content="Map Reduce Data Analysis" HorizontalAlignment="Left" Margin="460,60,0,0" VerticalAlignment="Top" Width="200" Height="80" IsDefault="True"/>
        <Border x:Name="textBlockBorder" BorderThickness="1" BorderBrush="Black" Width="700" Height="237" Margin="50,170,50,27">
            <TextBlock x:Name="consoleTextBlock" HorizontalAlignment="Center" Text="" TextWrapping="Wrap" VerticalAlignment="Center" Height="237" Width="700"/>
        </Border>
    </Grid>
</Window>
